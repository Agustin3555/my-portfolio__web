---
import * as ContactLinkStyled from "./ContactLink.styled";
import Icon from "../Icon/Icon.astro";
import { FONT_SIZE } from "@/styles";
import jsonData from "@/data.json";

const { sections } = jsonData.pages.home;
const sectionKey: keyof typeof sections = "contact";
---

<ContactLinkStyled.Component href={`/#${sectionKey}`}>
  <Icon iconName="fa-solid fa-paper-plane" style={{ size: FONT_SIZE.m }} />
</ContactLinkStyled.Component>
